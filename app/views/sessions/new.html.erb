<h1> Log In </h1>

<form action="<%= session_url %>" method="post">
  <label for="username">username</label>
  <input id="username" type="text" name="user[user_name]" placeholder="Username">

  <br>

  <label for="password">Password</label>
  <input id="password" type="password" name="user[password]" placeholder="******">

  <input type="submit">
</form>
